<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <!--注册标题文字-->
            <TextView
                android:id="@+id/textview_Baby"
                style="@style/header_bar"
                android:text="接种信息" />
            <!--边框分割细线-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/qmui_s_list_item_bg_1" />
            <!--输入框-->
            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/qmui_config_color_white"
                android:layout_margin="15dp"
                android:columnCount="2"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <!--接种地区-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill"
                    android:gravity="center"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:paddingRight="@dimen/qmui_content_spacing_horizontal"
                    android:text="接种地区"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--接种地区输入框-->
                <TextView
                    android:id="@+id/edittext_certiArea"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill"
                    android:hint="福建省厦门市湖里区"
                    android:background="@drawable/qmui_divider_bottom_bitmap"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:textColor="?attr/qmui_config_color_gray_5"
                    android:textSize="?attr/qmui_common_list_item_detail_h_text_size" />
                <!--宝宝姓名-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/qmui_s_list_item_bg_1"
                    android:gravity="center_vertical"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:paddingRight="@dimen/qmui_content_spacing_horizontal"
                    android:text="宝宝姓名"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--宝宝姓名输入框-->
                <TextView
                    android:id="@+id/edittext_babyName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill"
                    android:hint="南瓜"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:background="@drawable/qmui_divider_bottom_bitmap"
                    android:textColor="?attr/qmui_config_color_gray_5"
                    android:textSize="?attr/qmui_common_list_item_detail_h_text_size" />
                <!--宝宝出生日期-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/qmui_s_list_item_bg_1"
                    android:gravity="center_vertical"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:paddingRight="@dimen/qmui_content_spacing_horizontal"
                    android:text="出生日期"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--出生日期输入框-->
                <TextView
                    android:id="@+id/edittext_babyBirth"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/qmui_divider_bottom_bitmap"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:hint="2019.03.05"
                    android:layout_gravity="fill"
                    android:textColor="?attr/qmui_config_color_gray_5"
                    android:textSize="?attr/qmui_common_list_item_detail_h_text_size" />
                <!--宝宝性别-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/qmui_s_list_item_bg_1"
                    android:gravity="center_vertical"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:paddingRight="@dimen/qmui_content_spacing_horizontal"
                    android:text="宝宝性别"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size"/>
                <TextView
                    android:id="@+id/edittext_babySex"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/qmui_divider_bottom_bitmap"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:hint="女"
                    android:layout_gravity="fill"
                    android:textColor="?attr/qmui_config_color_gray_5"
                    android:textSize="?attr/qmui_common_list_item_detail_h_text_size" />
                <!--户口地址-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/qmui_s_list_item_bg_1"
                    android:gravity="center_vertical"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:paddingRight="@dimen/qmui_content_spacing_horizontal"
                    android:text="户口地址"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--户口地址输入框-->
                <TextView
                    android:id="@+id/edittext_homead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill"
                    android:background="@drawable/qmui_divider_bottom_bitmap"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:hint="福建省龙岩市"
                    android:textColor="?attr/qmui_config_color_gray_5"
                    android:textSize="?attr/qmui_common_list_item_detail_h_text_size" />
                <!--现居地址-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/qmui_s_list_item_bg_1"
                    android:gravity="center_vertical"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:paddingRight="@dimen/qmui_content_spacing_horizontal"
                    android:layout_marginBottom="18dp"
                    android:text="现居地址"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--现居地址输入框-->
                <TextView
                    android:id="@+id/edittext_nowad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill"
                    android:background="@drawable/qmui_divider_bottom_bitmap"
                    android:layout_marginBottom="18dp"
                    android:paddingTop="@dimen/qmui_content_spacing_horizontal"
                    android:hint="福建省厦门市"
                    android:textColor="?attr/qmui_config_color_gray_5"
                    android:textSize="?attr/qmui_common_list_item_detail_h_text_size" />

            </GridLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>