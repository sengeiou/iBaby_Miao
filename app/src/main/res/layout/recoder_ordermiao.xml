<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        style="@style/relative_bar"
        >
        <TextView
            android:id="@+id/comeBack"
            style="@style/img_back"/>
        <TextView
            android:id="@+id/textview_Yimiao"
            style="@style/top_bar"
            android:text="预约详情"
            />
    </RelativeLayout>

    <com.qmuiteam.qmui.widget.QMUIObservableScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!--边框分割细线-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/qmui_s_list_item_bg_1" />
            <!--输入框-->
            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/qmui_config_color_white"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:padding="15dp"
                android:columnCount="1"
                android:focusable="true"
                android:focusableInTouchMode="true">
            <!--疫苗名称-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="fill_horizontal"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:text="疫苗名称"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_name"
                style="@style/textShow"
                android:text="卡介苗"
                 />

            <!--疫苗简介-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:layout_marginTop="15dp"
                android:text="预防疾病"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_Effect"
                style="@style/textShow" />
            <!--适合年龄-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:layout_marginTop="15dp"
                android:text="适合接种时间"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_Age"
                style="@style/textShow" />
            <!--接种程序-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:text="接种程序"
                android:layout_marginTop="15dp"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_Process"
                style="@style/textShow" />
            <!--疫苗价格-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:text="疫苗价格"
                android:layout_marginTop="15dp"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_Price"
                style="@style/textShow" />
            <!--注意事项-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:text="注意事项"
                android:layout_marginTop="15dp"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_Attention"
                style="@style/textShow"/>
            <!--不良反应-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/qmui_s_list_item_bg_1"
                android:gravity="center_vertical"
                android:text="不良反应"
                android:layout_marginTop="15dp"
                android:textColor="?attr/qmui_config_color_gray_1"
                android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
            <!--输入框-->
            <TextView
                android:id="@+id/show_detail_miao_Disadv"
                style="@style/textShow" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/qmui_s_list_item_bg_1"
                    android:gravity="center_vertical"
                    android:layout_marginTop="15dp"
                    android:text="接种宝宝姓名"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--宝宝姓名输入框-->
                <TextView
                    android:id="@+id/show_OrderBabyName"
                    style="@style/textShow" />
                <!--接种地区-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill_horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginTop="15dp"
                    android:text="接种单位"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--接种地区输入框-->
                <TextView
                    android:id="@+id/show_OrderCertiArea"
                    style="@style/textShow" />

                <!--接种时间-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="fill_horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginTop="15dp"
                    android:text="预约接种时间"
                    android:textColor="?attr/qmui_config_color_gray_1"
                    android:textSize="?attr/qmui_common_list_item_title_h_text_size" />
                <!--接种地区输入框-->
                <TextView
                    android:id="@+id/show_OrderCertiTime"
                    style="@style/textShow" />

                <com.qmuiteam.qmui.layout.QMUIRelativeLayout
                    android:id="@+id/relative_show_other"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <GridLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:columnCount="3"
                        android:layout_marginTop="16dp"
                        android:layout_alignParentLeft="true">
                        <TextView
                            android:id="@+id/text_before"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:text="前方还有"
                            android:textSize="22sp"
                            />
                        <TextView
                            android:id="@+id/count_wait"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="5dp"
                            android:paddingRight="5dp"
                            android:layout_gravity="center_vertical"
                            android:text="5"
                            android:textSize="22sp"
                            android:textColor="@color/qmui_config_color_red"
                            />
                        <TextView
                            android:id="@+id/text_behind"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="人在等待"
                            android:textSize="22sp"
                            />
                    </GridLayout>

                </com.qmuiteam.qmui.layout.QMUIRelativeLayout>
                <com.qmuiteam.qmui.layout.QMUIRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="horizontal">

                    <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                        android:id="@+id/sure_Succeed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size"
                        app:qmui_isRadiusAdjustBounds="true"
                        android:layout_alignParentLeft="true"
                        android:padding="10dp"
                        android:text="确定完成"
                        />
                    <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                        android:id="@+id/sure_Cancel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size"
                        app:qmui_isRadiusAdjustBounds="true"
                        android:padding="10dp"
                        android:text="取消预约"
                        />
                </com.qmuiteam.qmui.layout.QMUIRelativeLayout>

            </GridLayout>
        </LinearLayout>
    </com.qmuiteam.qmui.widget.QMUIObservableScrollView>
</LinearLayout>